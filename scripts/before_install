#!/bin/bash
pwd
cd ../../examples/tutorial
easy_install pip
python3 -m venv /home/ubuntu/flask-test/venv
cd /home/ubuntu/flask-test
activate () {
    . /home/ubuntu/flask-test/venv/bin/activate
}

/home/ubuntu/flask-test/venv/bin/pip install -e ../..
/home/ubuntu/flask-test/venv/bin/pip install -e .